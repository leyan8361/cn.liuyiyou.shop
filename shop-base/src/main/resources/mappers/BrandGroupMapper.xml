<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.liuyiyou.shop.base.mapper.BrandGroupMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.liuyiyou.shop.base.entity.BrandGroup">
        <id column="id" property="id" />
        <result column="nav_id" property="navId" />
        <result column="group_name" property="groupName" />
        <result column="brand_array" property="brandArray" />
        <result column="status" property="status" />
        <result column="weight" property="weight" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, nav_id, group_name, brand_array, status, weight
    </sql>

</mapper>
